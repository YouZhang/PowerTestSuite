<?xml version="1.0" encoding="utf-8"?>
<Vp9_Power_Client_Config version="1.0.0">
	<Path>
        <AppPath>..\app</AppPath>
		<ClipsPath>..\Clips</ClipsPath>
		<LogPath>..\Log</LogPath>
		<RegPath>..\reg</RegPath>
		<RunListPath>RunList</RunListPath>
        <MVPLogPath>..\MVPLog</MVPLogPath>
        <batFilePath>bat</batFilePath>
	</Path>
    <Misc>
		<GPUReport>clipName_GPU.csv</GPUReport>
		<CPUReport>clipName_CPU.csv</CPUReport>
        <IP>10.239.141.154</IP>
        <Port>31500</Port>
	</Misc>
    <AppConfig>
        <fixedPlayback>
            <AppName>mv_decoder_adv</AppName>
            <AppPath>..\app</AppPath>
            <AppBin>mv_decoder_adv_1011.exe</AppBin>
            <Decoder>VP9</Decoder>
            <File>Input</File>
            <Display>cmd Fullscreen  overlay</Display>
            <Control>fps</Control>
        </fixedPlayback>
    </AppConfig>
    <Apps>
        <mv_decoder_adv>
            <Options>
                <Decoder>
                    <cmd></cmd>
                    <HEVC>--hevc</HEVC>
                    <VP9>--vp9</VP9>
                </Decoder>
                <File>
                    <cmd></cmd>
                    <Input cmd="-i">clipName</Input>
                    <Output cmd="-o">dumpFile</Output>
                </File>
                <Display>
                    <cmd>--disp</cmd>
                    <Fullscreen>--full</Fullscreen>
                    <Method>
                        <flip>--flip</flip>
                        <flipex>--flipex</flipex>
                        <overlay>--overlay</overlay>
                    </Method>
                </Display>
                <Control>
                    <cmd></cmd>
                    <length cmd="--limit">frameNum</length>
                    <fps cmd="--fps targetFPS ">
                        <F24>flim</F24>
                        <F30>ntsc</F30>
                        <F60>ntsc60</F60>
                        <F120>ntsc120</F120>
                        <free>asap</free>
                    </fps>
                </Control>
            </Options>
        </mv_decoder_adv>
    </Apps>
</Vp9_Power_Client_Config>